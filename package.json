{
  "name": "cordova-plugin-qrscanner-autofocus",
  "version": "3.2.0",
  "description": "Fast, energy-efficient, highly-configurable QR code scanner.",
  "cordova": {
    "id": "cordova-plugin-qrscanner-autofocus",
    "platforms": [
      "ios",
      "android"
    ]
  },
  "scripts": {
    "build": "gulp && npm run clean-build",
    "clean-build": "trash dist/plugin.min.js && trash dist/www.min.js && trash src/browser/worker.min.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/felicienfrancois/cordova-plugin-qrscanner-autofocus"
  },
  "keywords": [
    "cordova",
    "qr",
    "qr code",
    "scanner",
    "reader",
    "ecosystem:cordova",
    "cordova-ios",
    "cordova-android",
    "cordova-browser",
    "UMD",
    "library",
    "electron",
    "NW.js"
  ],
  "author": "Jason Dreyzehner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/felicienfrancois/cordova-plugin-qrscanner-autofocus/issues"
  },
  "homepage": "https://github.com/felicienfrancois/cordova-plugin-qrscanner-autofocus",
  "main": "dist/cordova-plugin-qrscanner-lib.min.js",
  "dependencies": {
    "qrcode-reader": "^1.0.4",
    "webrtc-adapter": "^3.1.4"
  },
  "devDependencies": {
    "gulp": "^4.0.2",
    "gulp-insert": "^0.5.0",
    "mkdirp": "^0.5.1",
    "raw-loader": "^0.5.1",
    "trash-cli": "^1.3.0",
    "validate-commit-msg": "^2.6.1",
    "webpack": "^2.7.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "helpMessage": "\nThis project uses commitizen to document changes. Please try:\nnpm install commitizen -g && git cz\n"
    }
  }
}
